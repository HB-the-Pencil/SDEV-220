    "# Section 4.1. I'd never heard of the Walrus operator before. (It works differently from Godot's.)\n",
    "secret = 5\n",
    "guess = int(input(\"Guess a number between 1 and 10: \"))\n",
    "\n",
    "if guess < secret:\n",
    "    print(\"Too low!\")\n",
    "elif guess > secret:\n",
    "    print(\"Too high!\")\n",
    "else:\n",
    "    print(\"Just right!\")"
   ]
